<div class="cardMd bdr-style-one">
  <div class="cardLog">
    <div class="cardTitle">
      <h2>Sign Up</h2>
    </div>
    <div id="signup" role="tabpanel">
      <form id="signup-form" class="cardLog bdr-style-one">
        <div>
          <label for="userSignup">Username</label>
          <input type="text" id="userSignup" class="input-box" />
        </div>
        <div>
          <label for="passSignup">Password</label>
          <input type="password" id="passSignup" class="input-box" />
        </div>
        <div class="flex-align-c">
          <button
            type="submit"
            id="signup-btn"
            class="all-btns link-btn-cta"
          >Sign Up</button>
          <a class="all-btns link-btn-cta msm" href="/login">Login</a>
        </div>
      </form>
    </div>
  </div>
</div>
<script src="/js/signup.js"></script>