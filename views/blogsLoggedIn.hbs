{{>newBlogBtn}}
<section id="blogs" class="container">
  <h1>All Blogs</h1>

  {{#each blogs}}
    <div class="blogCard bdr-style-one ">
      <a  class="blogTitle" href="/blogs/{{this.id}}">{{this.id}}. {{this.title}}
      </a>
     
      <p>{{this.contents}}</p>
       <div class="postedBy">posted by: {{user.username}} on 
    </div>
         </div>

  {{else}}
    <p>No blogs available</p>

  {{/each}}

</section>